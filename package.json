{
  "name": "code-collector-js",
  "displayName": "code-collector-js",
  "description": "Collects and displays content from all open tabs.",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./src/extension.js",
  "activationEvents": [
    "onCommand:code-collector-js.openPanel",
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "code-collector-js.openPanel",
        "title": "Open Code Collector Panel"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "webpack": "webpack --mode development",
    "release": "standard-version",
    "build": "cross-env NODE_ENV=production webpack",
    "watch": "cross-env NODE_ENV=development webpack --watch",
    "start": "cross-env NODE_ENV=development webpack serve",
    "vscode:prepublish": "npm run build",
    "package": "vsce package"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.1.0",
    "auto-changelog": "^2.5.0",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.1",
    "react-refresh": "^0.14.2",
    "standard-version": "^9.5.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patnaikd/code-collector-js.git"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^6.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
