{
  "name": "code-collector-js",
  "displayName": "code-collector-js",
  "description": "Collects and displays content from all open tabs.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:code-collector-js.openPanel",
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "code-collector-js.openPanel",
        "title": "Open Code Collector Panel"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^9.9.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}